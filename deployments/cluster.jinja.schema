info:
  title: GKE Cluster Template
  description: Creates a new XL ML Test cluster
  version: 0.1

properties:
  zone:
    type: string
    default: us-central1-b
    description: GCP zone for Kubernetes cluster

  huge-pool:
    type: boolean
    default: false
    description: Whether to include a pool of large GCE machines.

  double-v100:
    type: boolean
    default: false
    description: Whether to include a pool of GPU-enables machines.